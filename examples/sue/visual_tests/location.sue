# SUE version MMI_SUE4.4.0

proc SCHEMATIC_location {} {
  make input -name p0 -origin {0 0}
  make my_elem -orient R270 -name rotated -origin {40 190}
  make my_elem -orient R90X -name rotated_and_flipped -origin {150 -180}
  make my_elem -orient RY -name flipped -origin {-220 130}
  make my_elem -name nothing -origin {-210 -110}
  make_line 0 -190 0 180
  make_line -270 0 250 0
}
